.setSelectCell
  width 100%
  height 100%
  display flex
  &__grid
    width 150px
    height 150px
    margin auto
    border 2px solid #f1f1f1
    display grid
    grid-template-columns repeat(3, 1fr)
    grid-template-rows repeat(3, 1fr)
    grid-gap 5px
  &__item
    cursor pointer
    background-color #5e5e6c
    &.active
      background-color #306889
    &.current
      background-color #3e7c3a
      cursor default
